__lua__
function _init()
x=20
y=10
end

function _update()

if (btn(➡️)) x+=1
if (btn(⬅️)) x-=1
if (btn(⬆️)) y+=1 
if (btn(⬇️)) y-=1

end

function _draw()
cls()
map(0,0,0)
spr(4,x,y)

end 

-->8
// map

function draw_map () 
map (0,0,0,128,64)
end

function check_flag(flag,x,y)
local sprite=mget(x,y)
return fget(sprite,flag)
end


function old_camera()
camx= mid(0,p.x-7.5,31-15)
camy=mid(p.y-7.5/31-15)
camera(camx+8,camy+8)
end
